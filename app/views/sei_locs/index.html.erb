<h1>Listing sei_locs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sei_locs.each do |sei_loc| %>
      <tr>
        <td><%= sei_loc.name %></td>
        <td><%= link_to 'Show', sei_loc %></td>
        <td><%= link_to 'Edit', edit_sei_loc_path(sei_loc) %></td>
        <td><%= link_to 'Destroy', sei_loc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sei loc', new_sei_loc_path %>
